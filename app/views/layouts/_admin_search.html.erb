<div class="container">
<div class="search-form text-right">
  <%= form_with url: admin_search_path, method: :get, local: true do |f| %>
    <%= f.text_field :content %>
    <%= f.select :model, options_for_select({ "会員" => "customer", "商品名" => "product" }) %>
    <%= f.submit '検索' %>
  <% end %>
</div>
</div>